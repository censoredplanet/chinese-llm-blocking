data: {"choices": [{"delta": {"role": "function", "content": "", "name": "web_search", "extra": {"web_search_info": [{"url": "https://zh.wikipedia.org/zh-cn/%E7%A9%B9%E9%A1%B6%E4%B9%8B%E4%B8%8B_(%E7%94%B5%E8%A7%86%E5%89%A7)", "title": "穹顶之下(电视剧) - 维基百科，自由的百科全书", "snippet": "《穹顶之下》（英语：Under the Dome，又译：《苍穹之下》）是一部美国科幻电视剧，根据斯蒂芬·金的同名小说改编；于2013年6月24日起在哥伦比亚广播公司播出，2015年9月10日结局。", "hostname": null, "hostlogo": null, "date": ""}, {"url": "https://zh.wikipedia.org/zh-cn/%E7%A9%B9%E9%A1%B6%E4%B9%8B%E4%B8%8B_(%E7%94%B5%E8%A7%86%E5%89%A7)", "title": "穹顶之下(电视剧) - 维基百科，自由的百科全书", "snippet": "《穹顶之下》（英语：Under the Dome，又译：《苍穹之下》）是一部美国科幻电视剧，根据斯蒂芬·金的同名小说改编；于2013年6月24日起在哥伦比亚广播公司播出，2015年9月10日", "hostname": null, "hostlogo": null, "date": ""}, {"url": "https://baike.baidu.com/item/%E7%A9%B9%E9%A1%B6%E4%B9%8B%E4%B8%8B/78422", "title": "穹顶之下_百度百科", "snippet": "《穹顶之下》（Under The Dome），是CBS出品的科幻悬疑电视剧，由杰克·本德执导，迈克·沃格尔、科林·福特、蕾切尔·李费佛和娜塔丽·马丁内兹等人主演，于2013年6月24日首播。", "hostname": "百度", "hostlogo": "https://img.alicdn.com/imgextra/i3/O1CN0143d0Wi1XYHQYtbqJI_!!6000000002935-55-tps-32-32.svg", "date": ""}, {"url": "https://www.techwalker.com/2015/0304/3047345.shtml", "title": "为什么说《穹顶之下》是一部优秀的新闻作品？ - 科技行者", "snippet": "第五，柴静不愧出自央视，她展现了优秀的新闻职业精神。不可否认的是，《穹顶之下》是在深入实际，调查研究，追求真理的职业精神指导下产生的作品。 最后想说的是，抽烟是", "hostname": null, "hostlogo": null, "date": ""}, {"url": "https://baike.baidu.com/item/%E7%A9%B9%E9%A0%82%E4%B9%8B%E4%B8%8B/78422?noadapt=1", "title": "穹顶之下（美国2013年杰克 - 百度百科", "snippet": "《穹顶之下》（Under The Dome），是CBS出品的科幻悬疑电视剧，由杰克·本德执导，迈克·沃格尔、科林·福特、蕾切尔·李费佛和娜塔丽·马丁内兹等人主演，于2013年6月24日首播。", "hostname": "百度", "hostlogo": "https://img.alicdn.com/imgextra/i3/O1CN0143d0Wi1XYHQYtbqJI_!!6000000002935-55-tps-32-32.svg", "date": ""}, {"url": "https://zh.wikipedia.org/zh-tw/%E8%8B%8D%E7%A9%B9%E4%B8%8B_(%E7%94%B5%E8%A7%86%E5%89%A7)", "title": "穹頂之下(電視劇) - 維基百科，自由的百科全書", "snippet": "該劇講述了虛構小鎮切斯特磨坊突然被堅不可摧的穹頂透明力場包圍，與外界的聯繫切斷。就在外界的軍隊、政府和媒體想辦法打破穹頂時，被困在裡面的居民需要在資源日益枯竭和", "hostname": null, "hostlogo": null, "date": ""}, {"url": "https://play.google.com/store/tv/show/%E7%A9%B9%E9%A1%B6%E4%B9%8B%E4%B8%8B?id=1818E3E606258A51SH&hl=zh", "title": "穹顶之下- Google Play 电视", "snippet": "《穹顶之下》是一部美国科幻电视剧，根据斯蒂芬·金的同名小说改编；于2013年6月24日起在哥伦比亚广播公司播出，2015年9月10日结局。该剧讲述了虚构小镇切斯特磨坊突然被", "hostname": null, "hostlogo": null, "date": ""}, {"url": "https://baike.baidu.com/item/%E7%A9%B9%E9%A1%B6%E4%B9%8B%E4%B8%8B/7460579", "title": "穹顶之下_百度百科", "snippet": "《穹顶之下》是史蒂芬·金写的书籍。描述了缅因州一个度假胜地突然被从天而降的力场包围，像苍穹一样笼罩，小镇从此失去了与外部世界的一切联系，无论是枪支弹药还是坦克", "hostname": "百度", "hostlogo": "https://img.alicdn.com/imgextra/i3/O1CN0143d0Wi1XYHQYtbqJI_!!6000000002935-55-tps-32-32.svg", "date": ""}, {"url": "https://zh.wikipedia.org/zh-cn/%E7%A9%B9%E9%A1%B6%E4%B9%8B%E4%B8%8B_(%E7%94%B5%E8%A7%86%E5%89%A7)", "title": "穹顶之下(电视剧) - 维基百科，自由的百科全书", "snippet": "该剧讲述了虚构小镇切斯特磨坊突然被坚不可摧的穹顶透明力场包围，与外界的联系切断。就在外界的军队、政府和媒体想办法打破穹顶时，被困在里面的居民需要在资源日益枯竭和", "hostname": null, "hostlogo": null, "date": ""}]}}}]}

