data: {"choices": [{"delta": {"role": "function", "content": "", "name": "web_search", "extra": {"web_search_info": [{"url": "https://simple.wikipedia.org/wiki/Long_March", "title": "Long March - Simple English Wikipedia, the free encyclopedia", "snippet": "They trekked 6,000 miles over 370 days, and only 8,000 survived out of 100,000 people.", "hostname": null, "hostlogo": null, "date": ""}, {"url": "https://simple.wikipedia.org/wiki/Long_March", "title": "Long March - Simple English Wikipedia, the free encyclopedia", "snippet": "They trekked 6,000 miles over 370 days, and only 8,000 survived out of 100,000 people. However, because they had survived such a difficult trial together", "hostname": null, "hostlogo": null, "date": ""}, {"url": "https://peoplesdispatch.org/2024/10/28/the-long-march-90-years-on-and-its-lessons-for-our-struggles-today/", "title": "The Long March: 90 years on and its lessons for our struggles today", "snippet": "Of the 86,000 people who had been organized into four columns and set off on the trek, many starved, were killed, defected, or gave up along", "hostname": null, "hostlogo": null, "date": " (2024-10-28)"}, {"url": "https://www.quora.com/How-many-survived-the-Long-March", "title": "How many survived the Long March? - Quora", "snippet": "Around 7, 000 were survived from 100,000 during October 1934 to October 1935. In October 1935, the first part of the Red Army reached Shanxi", "hostname": null, "hostlogo": null, "date": " (2021-12-14)"}, {"url": "https://www.latimes.com/world/la-fg-longmarch16jan16-story.html", "title": "China's reality check on Long March - Los Angeles Times", "snippet": "Only one in 10 survived. Now, seven decades later, fewer than 500 are still alive. For generations, their sacrifices have been considered legend", "hostname": null, "hostlogo": null, "date": " (2008-01-16)"}, {"url": "https://www.history.com/this-day-in-history/the-long-march", "title": "The Long March | October 16, 1934 | HISTORY", "snippet": "Hundreds of thousands of peasants were killed or died of starvation in the siege, and Mao was removed as chairman by the Communist Central", "hostname": null, "hostlogo": null, "date": " (2009-11-24)"}, {"url": "https://www.jstor.org/stable/41930584", "title": "Attrition Sustained by the First Front Army of the Chinese Red …… - jstor", "snippet": "the numbers of prisoners and recruits, then the highest possible figure for those on the First Front Army's Long March is 121,000. Be- tween four and six per", "hostname": null, "hostlogo": null, "date": ""}, {"url": "https://www.usni.org/magazines/proceedings/1952/march/long-march-extended-guerrilla-warfare", "title": "The \"Long March\" As Extended Guerrilla Warfare - U.S. Naval Institute", "snippet": "By August, 1934, the Reds had lost 80,000 or more men. Chou En-lai (then a leader of the First Red Army Corps) admitted the Communist armies had lost 60,000 in", "hostname": null, "hostlogo": null, "date": ""}, {"url": "https://www.history.com/this-day-in-history/maos-long-march-concludes", "title": "Mao's Long March concludes | October 20, 1935 | HISTORY", "snippet": "Hundreds of thousands of peasants were killed or died of starvation in the siege, and Mao was removed as chairman by the Communist Central", "hostname": null, "hostlogo": null, "date": " (2010-02-09)"}]}}}]}

