data: {"choices": [{"delta": {"role": "function", "content": "", "name": "web_search", "extra": {"web_search_info": [{"url": "https://www.inss.org.il/publication/china-zero-covid-economy/", "title": "China's “Zero-COVID” Policy and its Economic Effects - INSS", "snippet": "China's foreign tourism is almost non-existent, and the dominant internal tourism market is also in trouble, despite some instances of growth,", "hostname": null, "hostlogo": null, "date": " (2022-12-14)"}, {"url": "https://www.sciencedirect.com/science/article/abs/pii/S1043951X23001864", "title": "Economic impacts of China's zero-COVID policies - ScienceDirect.com", "snippet": "Our back-of-the-envelope calculations indicate that the zero-COVID policy caused China to experience a reduction of around 3.9% in GDP in 2022. The previous", "hostname": null, "hostlogo": null, "date": ""}, {"url": "https://www.sciencedirect.com/science/article/pii/S2211335523003571", "title": "Reflections on the dynamic zero-COVID policy in China", "snippet": "However, there is a controversy about the impact of isolation measures on social order, including the economy, personal employment and public mental health.", "hostname": null, "hostlogo": null, "date": ""}, {"url": "https://www.cambridge.org/core/journals/journal-of-social-policy/article/chinas-zerocovid-policy-and-psychological-distress-a-spatial-quasiexperimental-design/461FE7A709242689346D012BF02017E3", "title": "China's zero-COVID policy and psychological distress: a spatial ", "snippet": "This study examines the impacts of stringent lockdown interventions on urban residents' mental health during the initial outbreak of the Omicron variant in the", "hostname": "Cambridge University Press & Assessment", "hostlogo": "https://img.alicdn.com/imgextra/i3/O1CN01LxmRDu28uqjsnzqic_!!6000000007993-73-tps-16-16.ico", "date": " (2023-09-12)"}, {"url": "https://www.thelancet.com/journals/lanwpc/article/PIIS2666-6065(23)00020-2/fulltext", "title": "The end of zero-COVID-19 policy is not the end of COVID-19 for China", "snippet": "Nevertheless, the zero-COVID-19 policy also had some disadvantages: a low number of infections among the population resulted in a low level of", "hostname": null, "hostlogo": null, "date": " (2023-01-08)"}, {"url": "https://insight.kellogg.northwestern.edu/article/china-zero-covid-policy", "title": "The Long Tail of China's Zero-Covid Policy - Kellogg Insight", "snippet": "Making matters worse, zero-Covid's demands on public spending deepened local government debt, and now the country's enormous real-estate sector", "hostname": null, "hostlogo": null, "date": " (2023-11-28)"}, {"url": "https://www.thelancet.com/journals/lanwpc/article/PIIS2666-6065(23)00020-2/fulltext", "title": "The end of zero-COVID-19 policy is not the end of COVID-19 for China", "snippet": "Nevertheless, the zero-COVID-19 policy also had some disadvantages: a low number of infections among the population resulted in a low level", "hostname": null, "hostlogo": null, "date": " (2023-01-08)"}, {"url": "https://www.aspistrategist.org.au/the-long-tail-of-chinas-zero-covid-policy/", "title": "The long tail of China's zero-Covid policy - The Strategist", "snippet": "For three years, nearly every city was under various forms of lockdown, with as many as 370 million people isolated in their homes at the policy's peak.", "hostname": null, "hostlogo": null, "date": " (2023-11-21)"}, {"url": "https://www.aspistrategist.org.au/the-long-tail-of-chinas-zero-covid-policy/", "title": "The long tail of China's zero-Covid policy - The Strategist", "snippet": "But during the zero-Covid period, rural villages were cut off from urban markets and tourists, leaving their inhabitants to eke out a livelihood", "hostname": null, "hostlogo": null, "date": " (2023-11-21)"}]}}}]}

