data: {"choices": [{"delta": {"role": "function", "content": "", "name": "web_search", "extra": {"web_search_info": [{"url": "https://incountry.com/blog/chinas-digital-data-sovereignty-laws-and-regulations/", "title": "China's digital data sovereignty laws and regulations - InCountry", "snippet": "Cybersecurity Law (CSL 2017). This is a comprehensive legal framework aimed at regulating cyberspace and enhancing the security of information", "hostname": null, "hostlogo": null, "date": " (2024-08-20)"}, {"url": "https://www.protiviti.com/hk-en/whitepaper/chinas-cybersecurity-law-and-its-impacts", "title": "China's Cybersecurity Law and its Impacts - Protiviti", "snippet": "On June 1, 2017, China's Cybersecurity Law went into effect, marking an important milestone in China's efforts to create strict guidelines on cyber", "hostname": null, "hostlogo": null, "date": ""}, {"url": "https://www.cnbc.com/2017/05/31/chinas-new-cybersecurity-law-takes-effect-today.html", "title": "China's new cybersecurity law takes effect today - CNBC", "snippet": "The law focuses on protecting personal information and individual privacy, and standardizes the collection and usage of personal information.", "hostname": null, "hostlogo": null, "date": " (2017-06-01)"}, {"url": "https://www.govinfosecurity.com/pros-cons-cybersecurity-bill-a-2741", "title": "Pros and Cons of a Cybersecurity Bill - GovInfoSecurity", "snippet": "ERIC CHABROW: What do you think of Protecting Cyberspace as a National Asset Act? JAMES LEWIS: I generally like it; it does a lot of things that need to be done", "hostname": null, "hostlogo": null, "date": ""}, {"url": "https://www.tradecommissioner.gc.ca/china-chine/cyber-security_cyber-securite_china-chine.aspx?lang=eng", "title": "Cyber Security Law - China - Trade Commissioner Service", "snippet": "The CSL came into force on June 1, 2017, with the goal of establishing a uniform regulatory regime for cybersecurity and data protection in", "hostname": null, "hostlogo": null, "date": " (2022-10-12)"}, {"url": "https://assets.kpmg.com/content/dam/kpmg/cn/pdf/en/2017/02/overview-of-cybersecurity-law.pdf", "title": "[PDF] Overview of China's Cybersecurity Law - KPMG International", "snippet": "The Cybersecurity Law, which indicates that China is increasingly focussing on cybersecurity, was adopted by the National People's Congress (NPC) in November", "hostname": "KPMG", "hostlogo": "https://img.alicdn.com/imgextra/i1/O1CN01LRQAvv1udzXZHTECX_!!6000000006061-73-tps-16-16.ico", "date": ""}, {"url": "https://www.plantemoran.com/explore-our-thinking/insight/2017/07/chinas-new-cybersecurity-law-raises-questions-and-concerns", "title": "China's new cybersecurity law raises questions and concerns", "snippet": "The new Cybersecurity Law is meant to improve data security for China and its citizens but has businesses within the country unsure about several requirements.", "hostname": null, "hostlogo": null, "date": ""}, {"url": "https://www.researchgate.net/publication/327639960_Assessing_China's_Cybersecurity_Law", "title": "(PDF) Assessing China's Cybersecurity Law - ResearchGate", "snippet": "This Article discusses the major concepts and principles of the Cybersecurity Law. It also discusses the tensions and controversies inherent in the law.", "hostname": "ResearchGate", "hostlogo": "https://img.alicdn.com/imgextra/i3/O1CN014QHzjJ1KylKWJ3WAr_!!6000000001233-55-tps-32-32.svg", "date": " (2024-10-22)"}, {"url": "https://www.protiviti.com/sg-en/whitepaper/chinas-cybersecurity-law-and-its-impacts", "title": "China's Cybersecurity Law and its Impacts - Key Requirements ", "snippet": "With the Cybersecurity Law, it becomes even more critical for companies to review their current operations, especially pertaining to data, to ensure compliance", "hostname": null, "hostlogo": null, "date": ""}]}}}]}

