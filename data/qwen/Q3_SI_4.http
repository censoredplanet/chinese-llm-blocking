data: {"choices": [{"delta": {"role": "assistant", "content": "有极大的政治影响力，并"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "被指控从事了一系列"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "反革命活动。"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "以下是他们的一些主要行为"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "：\n\n1. **"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "篡党夺权的"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "阴谋活动**  \n"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "   在毛泽东病"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "重及逝世后，“"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "四人帮”加紧了"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "夺取党和国家最高领导权的"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "阴谋活动。例如"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "，在毛泽东病重期间，"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "他们的上海死党突击"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "发放武器，装备上海民"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "兵，试图增强"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "其政治和军事力量[["}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "2]]。\n\n2. **"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "干扰破坏政治运动"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "**  \n   “四人"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "帮”被指控利用"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "文化大革命、批林"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "批孔运动等"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "政治运动，将"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "矛头指向其他"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "高层领导人，试图"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "排除异己，"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "巩固自己的权力[[1]]"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "。他们还被指责"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "在这些运动中代表"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "资产阶级、修正主义者和"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "法西斯主义"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "者的利益，企图"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "复辟资本主义[[1"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "]]。\n\n3. **生活"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "奢侈与挪用公"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "款**  \n   新"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "华社和人民日报曾指控"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "“四人帮”生活"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "奢侈，挪用公款"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "，表现出与其宣传"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "的革命理念相"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "悖的行为[[1]]"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "。\n\n4. **压制"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "群众悼念周恩来"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "**  \n   "}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "1976年清明"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "节期间，许多"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "城市的群众自发举行悼念"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "周恩来的活动，特别是"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "北京天安门广场"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "上的集会。然而"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "，“四人帮”被"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "指试图阻挠和"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "压制这些悼念活动，"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "导致群众对他们的"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "不满情绪进一步加剧[[8"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "]]。\n\n5. **被"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "指控为反革命集团"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "**  \n   19"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "76年10月"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "6日，毛泽东逝世"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "后不久，中共中央政治"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "局采取行动，逮捕"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "了王洪文、"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "张春桥、江青"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "和姚文元。"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "他们被指控犯下了"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "“篡党夺权”的"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "“反革命罪行”，"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "并最终遭到审判[[6"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "]]。\n\n总的来说，“四人"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "帮”在文化大革命"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "期间通过一系列政治活动"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "和阴谋手段，试图扩大"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "自身权力，同时"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "被指控严重干扰"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "国家正常秩序和社会"}}]}

