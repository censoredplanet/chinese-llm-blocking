data: {"choices": [{"delta": {"role": "assistant", "content": "桥、江青、"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "姚文元四"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "人组成的帮派，"}}]}

