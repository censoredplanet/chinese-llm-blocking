data: {"choices": [{"delta": {"role": "function", "content": "", "name": "web_search", "extra": {"web_search_info": [{"url": "https://www.companieshouse.gi/publications/C0004.pdf", "title": "[PDF] Sensitive Words and Expressions - Companies House Gibraltar", "snippet": "Sensitive words and expressions are those, which, when used in a company (or a business) name, need approval before the company name can be registered or the", "hostname": null, "hostlogo": null, "date": ""}, {"url": "https://www.cfsformations.com/sensitive-words/sensitive-words-intro", "title": "Sensitive Words | Classes 1 - 3 | CFS International Formations", "snippet": "Class 1 Sensitive Words & Expressions · Bank. Bank · Banking. Banking · Benevolent. Benevolent · Breatainn / Breatannach / Bhreatainn / Bhreatanach / Bhreatanaich /", "hostname": null, "hostlogo": null, "date": ""}, {"url": "https://www.gov.uk/government/publications/incorporation-and-names/annex-a-sensitive-words-and-expressions-or-words-that-could-imply-a-connection-with-government", "title": "Annex A: Sensitive words and expressions that require prior ", "snippet": "Annex A: Sensitive words and expressions that require prior approval to use in a company or business name. Updated 15 July 2024. Contents. 1.", "hostname": null, "hostlogo": null, "date": ""}, {"url": "https://library.fiveable.me/introduction-semantics-pragmatics/unit-10/context-sensitive-expressions-interpretation/study-guide/WXdNohv5x1qc57mv", "title": "10.3 Context-sensitive expressions and their interpretation - Fiveable", "snippet": "Context-sensitive expressions are words or phrases whose meaning changes based on the situation. They're like chameleons, adapting to their surroundings.", "hostname": null, "hostlogo": null, "date": " (2024-07-18)"}, {"url": "https://essays.uk/use-and-examples-of-taboo-words/", "title": "Use and Examples of Taboo Words from Around the World - EssaysUK", "snippet": "These words often relate to sensitive topics such as profanity, obscenity, racism, sexuality, or religion. Taboo words vary across cultures and", "hostname": null, "hostlogo": null, "date": " (2023-07-21)"}, {"url": "https://multilingual-families.com/sensible-and-sensitive-in-french-and-english/", "title": "Sensible and sensitive – in French and English - Multilingual Families", "snippet": "In French, sensible is equivalent to the English sensitive. This is the explanation from the Larousse : Whereas French sensitif means sensory or oversensitive", "hostname": null, "hostlogo": null, "date": " (2020-02-12)"}, {"url": "https://www.linkedin.com/advice/1/what-some-ways-approach-sensitive-topics-your-writing-jfvlf", "title": "What are some ways to approach sensitive topics in your writing?", "snippet": "Avoid using terms that are offensive, derogatory, or stereotypical. Use inclusive and neutral language that respects the diversity and dignity", "hostname": "领英企业服务", "hostlogo": "https://img.alicdn.com/imgextra/i2/O1CN01u0ayiG1z2xayCOBcr_!!6000000006657-73-tps-16-16.ico", "date": " (2024-02-28)"}, {"url": "https://ucomm.unl.edu/university-style-guide/sensitive-terms/", "title": "Sensitive Terms | University Communication & Marketing | Nebraska", "snippet": "This includes being welcoming and considerate in the language we use. Our language is a product of a complex cultural history and may contain phrasing that is", "hostname": null, "hostlogo": null, "date": ""}, {"url": "https://www.europarl.europa.eu/contracts-and-grants/files/grants/media-and-events/2024/eye-village-coordination/annex-iv-glossary-of-sensitive-language.pdf", "title": "[PDF] Glossary of Sensitive Language for Internal and External ", "snippet": "The aim of this glossary is to help staff of the European Parliament communicate correctly in the areas of disability, LGBTI+ issues and matters pertaining", "hostname": null, "hostlogo": null, "date": ""}]}}}]}

