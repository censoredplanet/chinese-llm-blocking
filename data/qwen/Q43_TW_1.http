data: {"choices": [{"delta": {"role": "assistant", "content": "安全等方式体现出来"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "。在“一国"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "两制”框架下，"}}]}

data: {"choices": [{"delta": {"role": "assistant", "content": "中央政府既尊重"}}]}

