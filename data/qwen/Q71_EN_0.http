data: {"choices": [{"delta": {"role": "function", "content": "", "name": "web_search", "extra": {"web_search_info": [{"url": "https://www.sciencedirect.com/science/article/abs/pii/S2211695822000897", "title": "Internet censorship, recoding, and the sensitive word culture in China", "snippet": "In China, these words are referred to as “sensitive words” (敏感词), and often include information such as political criticism, violence, and pornography.", "hostname": null, "hostlogo": null, "date": ""}, {"url": "https://www.companieshouse.gi/publications/C0004.pdf", "title": "[PDF] Sensitive Words and Expressions - Companies House Gibraltar", "snippet": "Sensitive words and expressions are those, which, when used in a company (or a business) name, need approval before the company name can be registered or the", "hostname": null, "hostlogo": null, "date": ""}, {"url": "https://www.cfsformations.com/sensitive-words/sensitive-words-intro", "title": "Sensitive Words | Classes 1 - 3 | CFS International Formations", "snippet": "Class 1 Sensitive Words & Expressions · Bank. Bank · Banking. Banking · Benevolent. Benevolent · Breatainn / Breatannach / Bhreatainn / Bhreatanach / Bhreatanaich /", "hostname": null, "hostlogo": null, "date": ""}, {"url": "https://library.fiveable.me/introduction-semantics-pragmatics/unit-10/context-sensitive-expressions-interpretation/study-guide/WXdNohv5x1qc57mv", "title": "10.3 Context-sensitive expressions and their interpretation - Fiveable", "snippet": "Context-sensitive expressions are words or phrases whose meaning changes based on the situation. They're like chameleons, adapting to their surroundings.", "hostname": null, "hostlogo": null, "date": " (2024-07-18)"}, {"url": "https://www.oed.com/information/editorial-policy/potentially-offensive-content/", "title": "Understanding sensitive and potentially offensive content", "snippet": "Sensitive content is anything that may cause offence to a reader, particularly in relation to religion, race, gender, politics, sexuality, disability.", "hostname": null, "hostlogo": null, "date": ""}, {"url": "https://essays.uk/use-and-examples-of-taboo-words/", "title": "Use and Examples of Taboo Words from Around the World - EssaysUK", "snippet": "These words often relate to sensitive topics such as profanity, obscenity, racism, sexuality, or religion. Taboo words vary across cultures and", "hostname": null, "hostlogo": null, "date": " (2023-07-21)"}, {"url": "https://multilingual-families.com/sensible-and-sensitive-in-french-and-english/", "title": "Sensible and sensitive – in French and English - Multilingual Families", "snippet": "In French, sensible is equivalent to the English sensitive. This is the explanation from the Larousse : Whereas French sensitif means sensory or oversensitive", "hostname": null, "hostlogo": null, "date": " (2020-02-12)"}, {"url": "https://www.frontiersin.org/journals/psychology/articles/10.3389/fpsyg.2023.1266425/pdf", "title": "[PDF] managing sensitive language in organizational communication", "snippet": "Three Linguistic Inquiry and Word Count (LIWC-22: Boyd et al., 2022) categories are used to provide an index of potentially sensitive language.", "hostname": null, "hostlogo": null, "date": " (2024-02-23)"}, {"url": "https://ucomm.unl.edu/university-style-guide/sensitive-terms/", "title": "Sensitive Terms | University Communication & Marketing | Nebraska", "snippet": "This includes being welcoming and considerate in the language we use. Our language is a product of a complex cultural history and may contain phrasing that is", "hostname": null, "hostlogo": null, "date": ""}]}}}]}

