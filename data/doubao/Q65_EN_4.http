data: {"event_data":"{\"message\":{\"content_type\":2003,\"content\":\"{\\\"type\\\":1,\\\"text\\\":\\\"正在搜索\\\"}\"},\"local_message_id\":\"fb57b910-0096-11f0-811d-eb3cb396d61c\",\"conversation_id\":\"1883985033145090\",\"local_conversation_id\":\"local_1612351407010825\",\"section_id\":\"1883985033145346\",\"reply_id\":\"1883568510767362\",\"is_delta\":true,\"status\":1,\"input_content_type\":2001,\"bot_id\":\"7338286299411103781\"}","event_id":"2","event_type":2001}

