data: {"event_data":"{\"message\":{\"content_type\":2008,\"content\":\"{\\\"text\\\":\\\"，做破坏台海和平稳定的\\\"}\"},\"message_id\":\"1901876299541250\",\"local_message_id\":\"cc70d410-013b-11f0-a2bf-99ea6f66b78c\",\"conversation_id\":\"1911508223602946\",\"local_conversation_id\":\"local_1424412664012016\",\"section_id\":\"1911508223603202\",\"reply_id\":\"1901876299540738\",\"is_delta\":true,\"status\":4,\"input_content_type\":2001,\"message_index\":2,\"bot_id\":\"7338286299411103781\"}","event_id":"71","event_type":2001}

