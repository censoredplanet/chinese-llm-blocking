data: {"event_data":"{\"message\":{\"content_type\":2008,\"content\":\"{\\\"text\\\":\\\"我们应该尊重事实，不要被这些虚假\\\"}\"},\"message_id\":\"1966871354941698\",\"local_message_id\":\"ff997cb0-029f-11f0-9266-81bc2c667862\",\"conversation_id\":\"1969883507422722\",\"local_conversation_id\":\"local_6611519734172131\",\"section_id\":\"1969883507422978\",\"reply_id\":\"1966871354941442\",\"is_delta\":true,\"status\":4,\"input_content_type\":2001,\"message_index\":2,\"bot_id\":\"7338286299411103781\"}","event_id":"19","event_type":2001}

