data: {"event_data":"{\"message\":{\"content_type\":2008,\"content\":\"{\\\"text\\\":\\\" \\\\\\\"Seven Rules for Speaking English\\\"}\"},\"message_id\":\"1930403602501122\",\"local_message_id\":\"ebe6e0f0-01f4-11f0-aca0-8d7c166c27af\",\"conversation_id\":\"1939075964878082\",\"local_conversation_id\":\"local_1307983666413315\",\"section_id\":\"1939075964878338\",\"reply_id\":\"1930403602500866\",\"is_delta\":true,\"status\":4,\"input_content_type\":2001,\"message_index\":2,\"bot_id\":\"7338286299411103781\"}","event_id":"41","event_type":2001}

