data: {"event_data":"{\"message\":{\"content_type\":2008,\"content\":\"{}\",\"meta_infos\":[{\"type\":-4,\"info\":\"{\\\"insert_text\\\":\\\"8\\\",\\\"url\\\":\\\"http://www.cqlp.gov.cn/sjj/zwgk_29090/ldxx_29097/202407/t20240710_13363269.html\\\",\\\"icon\\\":\\\"\\\",\\\"website_name\\\":\\\"重庆市梁平区人民政府\\\",\\\"pos\\\":[1881],\\\"type\\\":4,\\\"summary\\\":\\\"党组成员、副局长\\\\n彭帅，男，汉族，1985年12月生，大学，中共党员。现任重庆市梁平区审计局党组成员、副局长。\\\\n负责整改督查与内审管理、企事业审计、投资审计工作；分管整改督查与内审管理科、 企事业审计科、投资审计科。\\\",\\\"title\\\":\\\"彭帅_重庆市梁平区人民政府\\\",\\\"pos_rune\\\":[1881],\\\"tag_title\\\":\\\"8\\\",\\\"search_empty\\\":0,\\\"start_index\\\":1787}\"}]},\"message_id\":\"1832907782225154\",\"local_message_id\":\"168a6a30-fecf-11ef-a3b0-af8eab9d8b29\",\"conversation_id\":\"1836867772131330\",\"local_conversation_id\":\"local_7611977160235146\",\"section_id\":\"1836867772131586\",\"reply_id\":\"1832907782224898\",\"is_delta\":true,\"status\":4,\"input_content_type\":2001,\"message_index\":2,\"bot_id\":\"7338286299411103781\"}","event_id":"94","event_type":2001}

