data: {"event_data":"{\"message\":{\"content_type\":2008,\"content\":\"{}\",\"meta_infos\":[{\"type\":-4,\"info\":\"{\\\"insert_text\\\":\\\"1\\\",\\\"url\\\":\\\"https://m.kankanews.com/detail/PbwR0gYYqy4?isVertical=1\\\",\\\"icon\\\":\\\"https://p3-search.byteimg.com/img/labis/bab13d257eac0452a7a54cc081c36aa5~noop.jpeg\\\",\\\"website_name\\\":\\\"看看新闻\\\",\\\"pos\\\":[123],\\\"type\\\":4,\\\"summary\\\":\\\"全会决定，接受秦刚同志辞职申请，免去秦刚同志中央委员会委员职务。\\\\n全会审议并通过了中共中央军事委员会关于李尚福、李玉超、孙金明严重违纪违法问题的审查报告，确认中央政治局之前作出的给予李尚福、李玉超、孙金明开除党籍的处分。\\\\n\\\\u003ctable\\\\u003e\\\\n\\\\u003c/table\\\\u003e\\\",\\\"title\\\":\\\"视频 | 二十届三中全会公报发布 接受秦刚同志辞职申请_一眼看天下_看看新闻网\\\",\\\"pos_rune\\\":[123],\\\"tag_title\\\":\\\"1\\\",\\\"search_empty\\\":0,\\\"start_index\\\":81}\"}]},\"message_id\":\"1966806025457922\",\"local_message_id\":\"4b8c7370-02b4-11f0-9266-81bc2c667862\",\"conversation_id\":\"1969459141132546\",\"local_conversation_id\":\"local_7693567223621911\",\"section_id\":\"1969459141132802\",\"reply_id\":\"1966806025457666\",\"is_delta\":true,\"status\":4,\"input_content_type\":2001,\"message_index\":2,\"bot_id\":\"7338286299411103781\"}","event_id":"19","event_type":2001}

