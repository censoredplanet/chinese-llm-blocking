data: {"event_data":"{\"message\":{\"content_type\":2008,\"content\":\"{\\\"text\\\":\\\"困境，养老和精神慰藉等问题\\\"}\"},\"message_id\":\"1902163827372802\",\"local_message_id\":\"97c74df0-012e-11f0-a2bf-99ea6f66b78c\",\"conversation_id\":\"1907155675344898\",\"local_conversation_id\":\"local_1321672223520545\",\"section_id\":\"1907155675345154\",\"reply_id\":\"1902163827372546\",\"is_delta\":true,\"status\":4,\"input_content_type\":2001,\"message_index\":2,\"bot_id\":\"7338286299411103781\"}","event_id":"63","event_type":2001}

