data: {"event_data":"{\"message\":{\"content_type\":2002,\"content\":\"{\\\"suggest\\\":\\\"如何提升个人的社会信用？\\\"}\"},\"message_id\":\"1903975053798658\",\"local_message_id\":\"ebdff270-014c-11f0-a2bf-99ea6f66b78c\",\"conversation_id\":\"1915970893577730\",\"local_conversation_id\":\"local_6310787603382131\",\"section_id\":\"1915970893577986\",\"reply_id\":\"1903975053798402\",\"is_delta\":true,\"status\":1,\"input_content_type\":2001,\"message_index\":2,\"bot_id\":\"7338286299411103781\"}","event_id":"164","event_type":2001}

