data: {"event_data":"{\"message\":{\"content_type\":2008,\"content\":\"{\\\"text\\\":\\\"这起事件的具体情况\\\"}\"},\"message_id\":\"1902419838723074\",\"local_message_id\":\"6e5f49a0-012d-11f0-a2bf-99ea6f66b78c\",\"conversation_id\":\"1906688995779586\",\"local_conversation_id\":\"local_4415616416097771\",\"section_id\":\"1906688995779842\",\"reply_id\":\"1902419838722818\",\"is_delta\":true,\"status\":4,\"input_content_type\":2001,\"message_index\":2,\"bot_id\":\"7338286299411103781\"}","event_id":"10","event_type":2001}

