data:{"status":0,"qid":"7503199539047881102","pkgId":"52b4a9d9-c274-4974-b2e4-0b4f54ac44e0_4","sessionId":"7503199539047881102","isDefault":0,"isShow":0,"data":{"message":{"msgId":"52b4a9d9-c274-4974-b2e4-0b4f54ac44e0","isRebuild":false,"updateTime":"1742097158258","metaData":{"state":"generating-resp","endTurn":false,"userInfo":{"status":-1},"speedInfo":{}},"content":{"generator":{"text":"","type":"entry","dataType":"","showType":"","antiFlag":0,"isSafe":1,"isFinished":false,"component":"mindMap","group":1,"data":{"state":"no-mind-map","mindMapId":"7503199539047881102"},"needClearMindMap":true}}}},"seq_id":4,"product":""}


