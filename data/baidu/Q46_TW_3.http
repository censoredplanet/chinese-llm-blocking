data:{"status":0,"qid":"7034598055486378806","pkgId":"caa9a131-fd5c-41e4-b186-7870576dbed9_4","sessionId":"7034598055486378806","isDefault":0,"isShow":0,"data":{"message":{"msgId":"caa9a131-fd5c-41e4-b186-7870576dbed9","isRebuild":false,"updateTime":"1742100563390","metaData":{"state":"generating-resp","endTurn":false,"userInfo":{"status":-1},"speedInfo":{}},"content":{"generator":{"text":"","type":"entry","dataType":"","showType":"","antiFlag":0,"isSafe":1,"isFinished":false,"component":"mindMap","group":1,"data":{"state":"no-mind-map","mindMapId":"7034598055486378806"},"needClearMindMap":true}}}},"seq_id":4,"product":""}


