data:{"status":0,"qid":"11089323599441217410","pkgId":"3e049daa-e87f-43d8-8074-2fe2152c962d_3","sessionId":"11089323599441217410","isDefault":0,"isShow":0,"data":{"message":{"msgId":"3e049daa-e87f-43d8-8074-2fe2152c962d","isRebuild":false,"updateTime":"1742096872606","metaData":{"state":"generating-resp","endTurn":false,"userInfo":{"status":-1},"speedInfo":{}},"content":{"generator":{"text":"","type":"entry","dataType":"","showType":"","antiFlag":0,"isSafe":1,"isFinished":false,"component":"thinkingSteps","group":1,"data":{"status":"generating","title":"快速模式 思考中","titleIcon":"https://gips3.baidu.com/it/u=3993729449,4068610368&fm=3028&app=3028&f=PNG&fmt=auto&q=100&size=f48_48","couldExpand":false,"steps":[{"title":"理解问题","step_name":"queryAnalysis"}]}}}}},"seq_id":3,"product":""}


